
// 3.տպել բոլոր թվերի գումարը:
public class Main {
    public static void main(String[] args) {
        int a3[][] = {
                {1, 2, 3, 4},
                {4, 3, 2, 1},
                {5, 6, 7, 8},
        };
        int sum = 0;
        for (int i = 0; i < a3.length; i++) {
            for (int j = 0; j < a3[i].length; j++) {
                sum += a3[i][j];
            }

        }
        System.out.println(sum);
        System.out.println(1+2+3+4+4+3+2+1+5+6+7+8);
    }
}
//result =46
