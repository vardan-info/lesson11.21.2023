public class Main {
    public static void main(String[] args) {
        char[][] Name = new char[5][5];

        Name[0][0] = 'S';
        Name[0][1] = 'A';
        Name[0][2] = 'A';
        Name[0][3] = 'H';
        Name[0][4] = 'X';
        for (int i = 0; i < Name.length; i++) {
            for(int j=0; j<Name[i].length; j++){
                 System.out.print(Name[i][j] + " ");

        }


    }


}
    }


