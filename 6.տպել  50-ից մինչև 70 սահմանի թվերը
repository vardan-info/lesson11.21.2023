// 6.տպել  50-ից մինչև 70 սահմանի թվերը
public class Main {
    public static void main(String[] args) {
        int array[][] = {
                {1, 61, 0, 4},
                {4, 51, 6, 1},
                {27, 6, 50, 18},
        };
        int sum = 0;
        for (int i = 0; i < array.length; i++) {
            for (int j=0; j< array.length;j++){
                if(array[i][j]>50 && array[i][j]<70){
                    System.out.println(array[i][j]);
            }



                }

            }

        }


    }
